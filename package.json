{
  "name": "SJCPDataTransferApp",
  "version": "1.4.0",
  "description": "The St. Jude Cloud Data Transfer App, designed for getting data in the St. Jude Cloud easily and reliably.",
  "homepage": "https://github.com/stjude/sjcloud-data-transfer",
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/sjcloud-data-transfer"
  },
  "main": "app/main.js",
  "scripts": {
    "start": "electron app/main.js",
    "start:dev": "cross-env NODE_ENV=development AUTOUPDATE_ENABLED='false' CHROMIUM_MENU='true' electron app/main.js",
    "compile": "cross-env gulp compile",
    "dev:web": "webpack-dev-server --open --watch --config tasks/webpack.web.js",
    "develop": "cross-env NODE_ENV=development AUTOUPDATE_ENABLED='false' CHROMIUM_MENU='true' gulp develop",
    "dist:build-mac": "cross-env NODE_ENV=production build -m --x64 --ia32",
    "dist:build-linux": "cross-env NODE_ENV=production build -l AppImage deb rpm zip --x64 --ia32",
    "dist:build-win": "cross-env NODE_ENV=production build -w --x64 --ia32",
    "dist:release-mac": "cross-env NODE_ENV=production build -m --x64 --ia32 -p always",
    "dist:release-linux": "cross-env NODE_ENV=production build -l deb rpm zip --x64 --ia32 -p always",
    "dist:release-win": "cross-env NODE_ENV=production build -w --x64 --ia32 -p always",
    "pretty": "prettier --write --single-quote --trailing-comma \"all\" --bracket-spacing \"true\" \"src/**/*.{js,ts}\"",
    "test": "gulp test",
    "test:main": "jasmine-ts src/main/*.spec.ts",
    "nyc": "nyc gulp test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "lint-staged"
  },
  "author": "St. Jude Children's Research Hospital <clay.mcleod@stjude.org>",
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ],
    "*.ts": [
      "npm run pretty",
      "git add"
    ]
  },
  "dependencies": {
    "async": "^2.4.1",
    "bootstrap": "^4.1.1",
    "bootstrap-tour": "^0.12.0",
    "cross-fetch": "^2.2.1",
    "expand-home-dir": "0.0.3",
    "express": "^4.15.4",
    "fs-extra": "^6.0.1",
    "glob-to-regexp": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-jasmine": "^4.0.0",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-serve": "^1.4.0",
    "gulp-typescript": "^4.0.2",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "gulp-webpack": "^1.5.0",
    "gulp-webserver": "^0.9.1",
    "jquery": "^3.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.4",
    "merge-stream": "^1.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "pem": "^1.9.7",
    "quasar-extras": "^2.0.3",
    "quasar-framework": "^0.16.0",
    "request": "^2.85.0",
    "request-progress": "^3.0.0",
    "require-dir": "^1.0.0",
    "selfsigned": "^1.10.1",
    "spinkit": "^1.2.5",
    "tippy.js": "^2.5.2",
    "todc-bootstrap": "^3.3.7-3.3.13",
    "tree-kill": "^1.1.0",
    "vue-tippy": "^2.0.12",
    "webpack-target-electron-renderer": "^0.4.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.0",
    "@types/request": "^2.47.0",
    "@types/rimraf": "^2.0.2",
    "@types/tmp": "0.0.33",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "d3-selection": "^1.1.0",
    "electron": "~2.0.2",
    "electron-builder": "^20.15.1",
    "electron-packager": "^12.1.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "gulp-bump": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jasmine": "^3.1.0",
    "jasmine-core": "*",
    "jasmine-ts": "^0.2.1",
    "less": "^3.0.4",
    "lint-staged": "^7.1.3",
    "node-sass": "^4.9.0",
    "null-loader": "^0.1.1",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "tmp": "0.0.33",
    "ts-loader": "^4.3.1",
    "typescript": "^2.9.1",
    "url-loader": "^1.0.1",
    "vue": "^2.4.2",
    "vue-loader": "^15.2.4",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vue2-dropzone": "^3.2.2",
    "vuex": "^3.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-notifier": "^1.5.0"
  },
  "nyc": {
    "include": [
      "app/bin/backend/**/*.js",
      "app/bin/frontend/**/*.js"
    ],
    "exclude": [
      "gulpfile.js",
      "*.conf.js",
      "*.config.js"
    ]
  }
}
