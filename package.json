{
  "name": "SJCPDataTransferApp",
  "version": "1.3.0",
  "description":
    "The St. Jude Cloud Data Transfer App, designed for getting data in the St. Jude Cloud easily and reliably.",
  "homepage": "https://github.com/stjude/sjcloud-data-transfer",
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/sjcloud-data-transfer"
  },
  "main": "app/main.js",
  "scripts": {
    "start": "electron app/main.js",
    "start:dev":
      "cross-env NODE_ENV=development AUTOUPDATE_ENABLED='false' CHROMIUM_MENU='true' electron app/main.js",
    "compile": "cross-env gulp compile",
    "develop":
      "cross-env NODE_ENV=development AUTOUPDATE_ENABLED='false' CHROMIUM_MENU='true' gulp develop",
    "dist:mac": "cross-env NODE_ENV=production build -m --x64 --ia32",
    "dist:linux":
      "cross-env NODE_ENV=production build -l deb rpm zip --x64 --ia32",
    "dist:win": "cross-env NODE_ENV=production build -w --x64 --ia32",
    "dist:publish-unix":
      "cross-env NODE_ENV=production build -l deb rpm zip -m --x64 --ia32 -p always",
    "dist:publish-win":
      "cross-env NODE_ENV=production build -w --x64 --ia32 -p always",
    "pretty": "prettier --write --tab-width 2 \"app/src/**/*.{js,ts}\"",
    "test": "nyc gulp test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "lint-staged"
  },
  "author": "St. Jude Children's Research Hospital <clay.mcleod@stjude.org>",
  "build": {
    "appId": "cloud.stjude.dta",
    "productName": "SJCP Data Transfer",
    "dmg": {},
    "linux": {
      "category": "Utility",
      "target": ["AppImage", "deb"]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "lint-staged": {
    "*.js": ["npm run pretty", "git add"],
    "*.ts": ["npm run pretty", "git add"]
  },
  "dependencies": {
    "async": "^2.4.1",
    "bootstrap": "^3.3.7",
    "bootstrap-tour": "^0.11.0",
    "expand-home-dir": "0.0.3",
    "express": "^4.15.4",
    "fs-extra": "^4.0.3",
    "glob-to-regexp": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-jasmine": "^2.4.2",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-serve": "^1.4.0",
    "gulp-typescript": "^3.2.3",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "gulp-webpack": "^1.5.0",
    "gulp-webserver": "^0.9.1",
    "jquery": "^3.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "lodash": "^4.17.4",
    "merge-stream": "^1.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.19.3",
    "node-powershell": "^3.3.1",
    "pem": "^1.9.7",
    "quasar-extras": "^0.0.8",
    "quasar-framework": "^0.14.7",
    "require-dir": "^0.3.2",
    "selfsigned": "^1.10.1",
    "spinkit": "^1.2.5",
    "tippy.js": "^1.2.0",
    "todc-bootstrap": "^3.3.7-3.3.13",
    "tree-kill": "^1.1.0",
    "vue-tippy": "^0.2.8",
    "webpack": "^3.8.1",
    "webpack-target-electron-renderer": "^0.4.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.3",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "d3-selection": "^1.1.0",
    "electron": "~1.8.4",
    "electron-builder": "^19.37.1",
    "electron-packager": "^9.1.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-google": "^0.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gulp-bump": "^2.9.0",
    "husky": "^0.14.3",
    "jasmine": "^2.8.0",
    "jasmine-core": "*",
    "jasmine-ts": "^0.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "null-loader": "^0.1.1",
    "nyc": "^11.3.0",
    "style-loader": "^0.18.2",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "url-loader": "^0.5.9",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.2",
    "vue2-dropzone": "^2.3.5",
    "vuex": "^2.3.1",
    "webpack-notifier": "^1.5.0"
  },
  "nyc": {
    "include": ["app/bin/backend/**/*.js", "app/bin/frontend/**/*.js"],
    "exclude": ["gulpfile.js", "*.conf.js", "*.config.js"]
  }
}
